{
    "description": "FGR Arcad Demo",
    "version": "0.0.1",
    "includePath": [
        "FGRMRL_SRC/QPROTOSRC"
    ],
    "repository": "git@github.com:fgrumeau/FGRMRL.git",
    "objlib": "&verlibfgrmrl",
    "curlib": "*CRTDFT",
